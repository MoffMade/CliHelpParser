version 1.0

task {{ taskname }} {
  input {
    {% for flag in positional -%}
        {{ flag.get_type() | type_to_wdl(false) }} {{ flag | choose_variable_name('snake')}}
    {% endfor -%}
    {% for flag in named -%}
      {{ flag.args.get_type() | type_to_wdl(true) }} {{ flag | choose_variable_name('snake')}}
    {% endfor -%}
  }

  command {
    {{ command }}
  }

  parameter_meta {
    {% for flag in named -%}
        {{ flag | choose_variable_name('snake')}}: "{{ flag.description | sanitize_str }}"
    {% endfor -%}
  }

  runtime {
  }

  output {
  }
}
